<suite name="ParallelTests" verbose="5" parallel="tests" thread-count="10">
    <test name="WebDriver">
        <parameter name="os" value="Windows 2003"/>
        <parameter name="browser" value="firefox"/>
        <parameter name="browserVersion" value="3.6."/>
        <classes>
            <class name="org.urbandaddy.helpers.Comm.iSauceTest"/>
        </classes>
    </test>
    <listeners>
        <listener class-name="saucelabs.testng.SauceOnDemandTestListener"/>
    </listeners>
</suite>